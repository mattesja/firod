{
  "name": "fiori_elements_tutorial",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4",
    "@sap/xssec": "^4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "npx cds run",
    "watch": "npx cds watch",
    "watch-mydemo": "cds watch --open my.demo.mydemo/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sql"
      },
      "[production]": {
        "auth": "xsuaa"
      },
      "destinations": true,
      "html5-runtime": true,
      "workzone": true
    },
    "odata": {
      "version": "v4"
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/mydemo"
  ]
}
